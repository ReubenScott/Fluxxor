{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch firefox",
      "type": "firefox",
      "request": "launch",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}",
      "profile": "default",
      "firefoxExecutable": "D:/Application/Browser/Firefox/firefox.exe",
      "firefoxArgs": [
        "-start-debugger-server",
        "-no-remote"
      ],
      "log": {
        "consoleLevel": {
          "PathConversion": "Warn",
          "default": "Warn"
        }
      },
      "pathMappings": [
        {
          "url": "file:///E:/Studio/VSCode/ReactDemo/Fluxxor/src",
          "path": "${workspaceFolder}/src"
        }
      ]
    }
  ]
}